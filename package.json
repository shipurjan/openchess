{
  "name": "openchess",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "dev": "tsx watch server.ts",
    "services": "docker compose up db redis -d --wait",
    "services:stop": "docker compose down",
    "db": "docker compose up db -d --wait",
    "db:stop": "docker compose down",
    "db:migrate": "prisma migrate deploy",
    "build": "next build",
    "start": "NODE_ENV=production tsx server.ts",
    "lint": "eslint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "docker:preview": "docker build -t ttl.sh/openchess:1h . && docker push ttl.sh/openchess:1h",
    "docker:run": "docker compose -f docker-compose.yml -f docker-compose.preview.yml up",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/adapter-pg": "7.4.0",
    "@prisma/client": "7.4.0",
    "chess.js": "1.4.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "dotenv": "17.3.1",
    "ioredis": "5.9.3",
    "lucide-react": "0.564.0",
    "next": "16.1.6",
    "pg": "8.18.0",
    "radix-ui": "1.4.3",
    "react": "19.2.4",
    "react-chessboard": "5.10.0",
    "react-dom": "19.2.4",
    "tailwind-merge": "3.4.1",
    "tsx": "4.21.0",
    "ws": "8.19.0"
  },
  "devDependencies": {
    "@playwright/test": "1.58.2",
    "@tailwindcss/postcss": "4.1.18",
    "@types/node": "24",
    "@types/pg": "8.16.0",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@types/ws": "8.18.1",
    "esbuild": "0.27.3",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.6",
    "jsdom": "28.1.0",
    "prisma": "7.4.0",
    "shadcn": "3.8.5",
    "tailwindcss": "4.1.18",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  },
  "packageManager": "pnpm@10.29.3+sha512.498e1fb4cca5aa06c1dcf2611e6fafc50972ffe7189998c409e90de74566444298ffe43e6cd2acdc775ba1aa7cc5e092a8b7054c811ba8c5770f84693d33d2dc",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/engines",
      "esbuild",
      "msw",
      "prisma"
    ]
  }
}
